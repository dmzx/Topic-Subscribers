	"version": "1.0.2",
	"time": "2016-09-25",
	"keywords": ["phpbb", "extension", "topicsubscribers"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
